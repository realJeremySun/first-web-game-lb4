{
  "name": "firstgame",
  "version": "1.0.0",
  "description": "firstgame",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc --outDir dist",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t firstgame .",
    "docker:run": "docker run -p 3000:3000 -d firstgame",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^2.1.8",
    "@loopback/boot": "^1.5.0",
    "@loopback/context": "^1.21.1",
    "@loopback/core": "^1.9.0",
    "@loopback/openapi-v3": "^1.9.1",
    "@loopback/repository": "^1.10.1",
    "@loopback/rest": "^1.16.6",
    "@loopback/rest-explorer": "^1.3.1",
    "@loopback/service-proxy": "^1.3.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/lodash": "^4.14.136",
    "@types/uuid": "^3.4.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "loopback-connector-mongodb": "^4.2.0",
    "npm-check": "^5.9.0",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@loopback/build": "^2.0.6",
    "@loopback/testlab": "^1.7.2",
    "@loopback/tslint-config": "^2.1.0",
    "@types/node": "^10.14.14",
    "@types/passport": "^1.0.0",
    "@types/passport-http": "^0.3.8",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  }
}
